// Retiré du code car réaffectation
x = 0
// Optimisation a x = 0, retiré du code, x deja = 0
x = x * 0
// Optimisation a x = x puis retire du code
x = x * 1
// Optimisation a x = 0, retiré du code, x deja = 0
x = 0 * x
// Optimisation a x = x, puis retire du code
x = 1 * x
// Pas d'optimisation
x = 10
// Pas d'optimisation
x = x * 3
// Pas d'optimisation
x = 3 * x